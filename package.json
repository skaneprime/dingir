{
  "name": "dingir",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "bin": "./bin/dingir.js",
  "typings": "./bin/dingir.d.ts",
  "scripts": {
    "extract-api": "npx api-extractor run --local --verbose & ts-node scripts/api-extractor-globalize.ts",
    "bundle": "npx ncc build bin/.tsc/index.js -o bin -e bytenode -e express -e typescript -e vm2 -e ts-node -e stacktracey -e @vercel/ncc -e @microsoft/api-extractor -m --target es2020 -C",
    "build": "npm run checkup & npm run clear & tsc & npm run bundle & npm run extract-api & npm run rename & npm run post-clear & pkg -C GZip .",
    "rename": "ts-nide scripts/rename.ts",
    "post-clear": "rd /s /q bin\\.tsc",
    "clear": "rd /s /q bin",
    "pretty": "prettier --write src/**/*.ts",
    "eslint": "eslint . --ext .ts",
    "checkup": "npm run pretty & npm run eslint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "eslint": "^8.8.0",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "@microsoft/api-extractor": "^7.19.4",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.12",
    "@types/require-from-string": "^1.2.1",
    "@vercel/ncc": "^0.33.1",
    "bytenode": "^1.3.4",
    "chalk": "^4.1.2",
    "commander": "^8.3.0",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "require-from-string": "^2.0.2",
    "source-map-support": "^0.5.21",
    "stacktracey": "^2.1.8",
    "ts-node": "^10.4.0",
    "tsc-prog": "^2.2.1",
    "typescript": "^4.5.5",
    "vm2": "^3.9.5"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "targets": [
      "node16-linux",
      "node16-win"
    ],
    "name": "dingir",
    "outputPath": "bin",
    "compress": "GZip"
  }
}
